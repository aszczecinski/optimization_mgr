CREATE TABLE "ELEMENT_ZAMOWIENIA"
 ( "ID" NUMBER(10,0) NOT NULL,
 "ID_ZAMOWIENIE" NUMBER(10,0) NOT NULL,
 "NAZWA_PRODUKTU" VARCHAR2(200) NOT NULL,
 "LICZBA" NUMBER(6,0) NOT NULL,
 "CENA_JEDNOSTKOWA_NETTO" NUMBER(24,2) NOT NULL,
 "STAWKA_VAT" NUMBER(3,2) NOT NULL,
 "UPUST" NUMBER(3,2),
 CONSTRAINT PK_ELZAM PRIMARY KEY (ID),
 CONSTRAINT FK_ELZAM FOREIGN KEY (ID_ZAMOWIENIE)
  REFERENCES "ZAMOWIENIE"(ID),
 CONSTRAINT EL_ZAM_UQ UNIQUE (ID_ZAMOWIENIE, NAZWA_PRODUKTU)
) ;

 COMMENT ON COLUMN "ELEMENT_ZAMOWIENIA"."ID" IS 'ID';
 COMMENT ON COLUMN "ELEMENT_ZAMOWIENIA"."ID_ZAMOWIENIE" IS 'ID zamówienia';
 COMMENT ON COLUMN "ELEMENT_ZAMOWIENIA"."NAZWA_PRODUKTU" IS 'Nazwa produktu w zamówieniu';
 COMMENT ON COLUMN "ELEMENT_ZAMOWIENIA"."LICZBA" IS 'Liczba fakturowanego produktu';
 COMMENT ON COLUMN "ELEMENT_ZAMOWIENIA"."CENA_JEDNOSTKOWA_NETTO" IS 'Cena netto za jeden produktu';
 COMMENT ON COLUMN "ELEMENT_ZAMOWIENIA"."STAWKA_VAT" IS 'Stawka vat - wyrażona w postaci liczby [x.xx]';
 COMMENT ON COLUMN "ELEMENT_ZAMOWIENIA"."UPUST" IS 'Upust do elementu faktury. Wartość procentowa, wyrażona w postaci liczby [x.xx]';

 COMMENT ON TABLE "ELEMENT_ZAMOWIENIA" IS 'Tabela przechowuje dane elementu faktury';