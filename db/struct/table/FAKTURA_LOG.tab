CREATE TABLE "FAKTURA_LOG"
 ( "ID_LOG" NUMBER(22,0) NOT NULL ENABLE,
 "DATA_UTWORZENIA" TIMESTAMP (6) NOT NULL ENABLE,
 "ID_FAKTURA" NUMBER(10,0),
 CONSTRAINT PK_LOG PRIMARY KEY (ID_LOG),
 CONSTRAINT FK_FLOG_FAK FOREIGN KEY (ID_FAKTURA)
 REFERENCES "FAKTURA"(ID)
 ) ;

 COMMENT ON COLUMN "FAKTURA_LOG"."ID_LOG" IS 'ID Log';
 COMMENT ON COLUMN "FAKTURA_LOG"."DATA_UTWORZENIA" IS 'Informacja kiedy faktura została zapisana w systemie';
 COMMENT ON COLUMN "FAKTURA_LOG"."ID_FAKTURA" IS 'ID Faktury, która została właśnie dodana';

 COMMENT ON TABLE "FAKTURA_LOG" IS 'Tabela przechowuje informacje na temat dat utworzenia faktur';

 CREATE INDEX "IDX_LOG_DATA_UTW" ON "FAKTURA_LOG" ("DATA_UTWORZENIA");
 CREATE INDEX "IDX_LOG_ID_FAKT" ON "FAKTURA_LOG" ("ID_FAKTURA");