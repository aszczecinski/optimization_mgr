CREATE TABLE "ELEMENT_FAKTURY"
 ( "ID" NUMBER(5,0) NOT NULL,
 "ID_FAKTURA" NUMBER(10,0) NOT NULL,
 "NAZWA_PRODUKTU" VARCHAR2(200) NOT NULL,
 "LICZBA" NUMBER(6,0) NOT NULL,
 "CENA_JEDNOSTKOWA_NETTO" NUMBER(24,2) NOT NULL,
 "STAWKA_VAT" NUMBER(3,2) NOT NULL,
 "UPUST" NUMBER(3,2),
 CONSTRAINT PK_ELFAKT PRIMARY KEY (ID),
 CONSTRAINT FK_EL_FAK_FAK FOREIGN KEY (ID_FAKTURA)
 REFERENCES "FAKTURA"(ID)
) ;

 COMMENT ON COLUMN "ELEMENT_FAKTURY"."ID" IS 'ID';
 COMMENT ON COLUMN "ELEMENT_FAKTURY"."ID_FAKTURA" IS 'ID faktury';
 COMMENT ON COLUMN "ELEMENT_FAKTURY"."NAZWA_PRODUKTU" IS 'Nazwa produktu na fakturze';
 COMMENT ON COLUMN "ELEMENT_FAKTURY"."LICZBA" IS 'Liczba fakturowanego produktu';
 COMMENT ON COLUMN "ELEMENT_FAKTURY"."CENA_JEDNOSTKOWA_NETTO" IS 'Cena netto za jeden produktu';
 COMMENT ON COLUMN "ELEMENT_FAKTURY"."STAWKA_VAT" IS 'Stawka vat - wyrażona w postaci liczby [x.xx]';
 COMMENT ON COLUMN "ELEMENT_FAKTURY"."UPUST" IS 'Upust do elementu faktury. Wartość procentowa, wyrażona w postaci liczby [x.xx]';

 COMMENT ON TABLE "ELEMENT_FAKTURY" IS 'Tabela przechowuje dane elementu faktury';