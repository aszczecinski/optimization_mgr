CREATE TABLE "WYSYLKA"
 ( "ID" NUMBER(5,0) NOT NULL,
 "ID_ZAMOWIENIE" NUMBER(10,0) NOT NULL,
 "ID_FAKTURA" NUMBER(10,0) NOT NULL,
 "ULICA_NUMER" VARCHAR2(200) NOT NULL ENABLE,
 "KOD_MIASTO" VARCHAR2(6) NOT NULL ENABLE,
 "MIASTO" VARCHAR2(200) NOT NULL ENABLE,
 "ID_DOSTAWCA" NUMBER(10,0) NOT NULL,
 "TYP" NUMBER(1,0) NOT NULL,
 CONSTRAINT PK_WYSYLKA PRIMARY KEY (ID),
 FOREIGN KEY (ID_ZAMOWIENIE)
 REFERENCES "ZAMOWIENIE"(ID)
) ;

 COMMENT ON COLUMN "WYSYLKA"."ID" IS 'ID wysyłki';
 COMMENT ON COLUMN "WYSYLKA"."ID_ZAMOWIENIE" IS 'ID zamówienia';
 COMMENT ON COLUMN "WYSYLKA"."ID_FAKTURA" IS 'ID faktury';
 COMMENT ON COLUMN "WYSYLKA"."ULICA_NUMER" IS 'Ulica oraz numer odbiorcy';
 COMMENT ON COLUMN "WYSYLKA"."KOD_MIASTO" IS 'Kod pocztowy (format XXXXX)';
 COMMENT ON COLUMN "WYSYLKA"."MIASTO" IS 'Nazwa miasta, do którego kierowana jest przesyłka';
 COMMENT ON COLUMN "WYSYLKA"."ID_DOSTAWCA" IS 'ID Dostawcy';
 COMMENT ON COLUMN "WYSYLKA"."TYP" IS 'Typ wysyłki: 0 - odbiór w punkcie, 1 - kurier, 2 - listy, 3 - dowóz własny';

 COMMENT ON TABLE "WYSYLKA" IS 'Tabela przechowuje dane dotyczące wysyłki zamówienia';